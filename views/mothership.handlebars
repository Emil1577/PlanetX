<div class="moon-container">
<h3>Explored Moons:</h3>
{{#if blogposts.length}}
{{#each blogposts as |moon|}}
      <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">{{moon.title}}</span>
          <p>{{moon.conent}}</p>
        </div>
        <div class="card-action">
          <a href="#">Rediscover</a>
          <a href="#">Destroy</a>
        </div>
      </div>
    </div>
  </div>
{{/each}}
{{else}}
<p>Go Out And Explore!</p>
{{/if}}
</div>